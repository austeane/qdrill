{
	"name": "sveltekit-2",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "vercel --prod",
		"tailwind:build": "tailwindcss build src/routes/styles.css -o public/build/tailwind.css",
		"tailwind:watch": "tailwindcss build src/routes/styles.css -o public/build/tailwind.css --watch"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.14",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.46.1",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"@sveltejs/kit": "^2.5.24",
		"@types/cookie": "^0.6.0",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.13.3",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"pnpm": "^9.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.5.4",
		"vite": "^5.4.2",
		"vitest": "^2.0.5",
		"web-vitals": "^4.2.3"
	},
	"dependencies": {
		"@mapbox/node-pre-gyp": "^1.0.11",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@sveltejs/vite-plugin-svelte-inspector": "^2.1.0",
		"@types/node": "^22.5.0",
		"@vercel/edge-config": "^1.2.1",
		"@vercel/postgres": "^0.9.0",
		"@zerodevx/svelte-toast": "^0.9.5",
		"aws-sdk": "^2.1683.0",
		"fabric": "^6.3.0",
		"mock-aws-s3": "^4.0.2",
		"nock": "^13.5.5",
		"pg": "^8.12.0",
		"svelte-dnd-action": "^0.9.50",
		"svelte-feather-icons": "^4.1.0",
		"svelte-forms-lib": "^2.0.1",
		"svelte-routing": "^2.13.0"
	},
	"type": "module"
}
