{
	"name": "sveltekit-2",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "vercel --prod",
		"tailwind:build": "tailwindcss build src/routes/styles.css -o public/build/tailwind.css",
		"tailwind:watch": "tailwindcss build src/routes/styles.css -o public/build/tailwind.css --watch"
	},
	"devDependencies": {
		"@auth/pg-adapter": "^1.7.1",
		"@fontsource/fira-mono": "^5.1.0",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.48.1",
		"@sveltejs/adapter-vercel": "^5.4.5",
		"@sveltejs/kit": "^2.7.1",
		"@types/cookie": "^0.6.0",
		"autoprefixer": "^10.4.20",
		"cypress": "^13.15.0",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"pnpm": "^9.12.2",
		"postcss": "^8.4.47",
		"postcss-nesting": "^13.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.5",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
		"vite": "^5.4.9",
		"vitest": "^2.1.3",
		"web-vitals": "^4.2.3"
	},
	"dependencies": {
		"@auth/core": "^0.37.1",
		"@auth/sveltekit": "^1.7.1",
		"@dnd-kit/core": "^6.2.0",
		"@excalidraw/excalidraw": "^0.17.6",
		"@mapbox/node-pre-gyp": "^1.0.11",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@sveltejs/vite-plugin-svelte-inspector": "^2.1.0",
		"@types/node": "^22.7.6",
		"@vercel/edge-config": "^1.3.0",
		"@vercel/postgres": "^0.10.0",
		"@zerodevx/svelte-toast": "^0.9.6",
		"aws-sdk": "^2.1691.0",
		"csv-parse": "^5.5.6",
		"fabric": "^6.4.3",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"mock-aws-s3": "^4.0.2",
		"nock": "^13.5.5",
		"pg": "^8.13.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"svelte-dnd-action": "^0.9.51",
		"svelte-feather-icons": "^4.1.0",
		"svelte-forms-lib": "^2.0.1",
		"svelte-range-slider-pips": "^3.1.1",
		"svelte-routing": "^2.13.0",
